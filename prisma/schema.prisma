generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum AuthProvider {
  Google
  Email
  LinkedIn
}

model User {
  id        Int          @id @default(autoincrement())
  name      String
  email     String       @unique
  password  String?
  provider  AuthProvider
  createdAt DateTime     @default(now())
}

